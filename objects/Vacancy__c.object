    <validationRules>
        <fullName>Parttime_Validation_rule</fullName>
        <active>true</active>
        <description>If the vacancy type is Part-time, then the duration should be mentioned between 1-365</description>
        <errorConditionFormula>ISPICKVAL( Vacancy_Type__c  ,&quot;Part-Time&quot;)   &amp;&amp;  (BLANKVALUE (Duration__c, 0)   &lt;=  0  ||  Duration__c  &gt; 365 )</errorConditionFormula>
        <errorMessage>Please fill in the duration between 1 and 365</errorMessage>
    </validationRules>
