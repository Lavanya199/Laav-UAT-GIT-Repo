<apex:page controller="ac3" >
  <apex:pageBlock title="Direct customers">
      <apex:pageBlockTable value="{!direct_customers}" var="a">
          <apex:column value="{!a.Name}"/>
          <apex:column value="{!a.Type}"/>
          <apex:column value="{!a.Industry}"/>
          <apex:column value="{!a.Phone}"/>
      </apex:pageBlockTable>
  </apex:pageBlock>
  
  
  
  <apex:pageBlock title="Channel Customers">
      <apex:pageBlockTable value="{!channel_customers}" var="a">
          <apex:column value="{!a.Name}"/>
          <apex:column value="{!a.Type}"/>
          <apex:column value="{!a.Industry}"/>
          <apex:column value="{!a.Phone}"/>
      </apex:pageBlockTable>
  </apex:pageBlock>
  
</apex:page>